int subj_1,subj_2,subj_3,subj_4,subj_5
read subj_1,subj_2,subj_3,subj_4,subj_5
sum_average = subj_1+subj_2+subj_3+subj_4+subj_5 / 5
print sum_average
if sum_average >= 80 then
print("excellent")
else
if sum_average >= 75 then
print("very good")
else 
if sum_average >= 65 then 
print("good")
else
print("bad")
